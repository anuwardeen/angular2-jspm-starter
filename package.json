  "repository": {
    "type": "git",
    "url": "git+https://github.com/samouss/angular2-jspm-starter.git"
  },
  "author": "Samuel Vaillant <samuel.vllnt@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samouss/angular2-jspm-starter/issues"
  },
  "homepage": "https://github.com/samouss/angular2-jspm-starter#readme",
  "engines": {
    "node": ">=v4.0.0",
    "npm": ">=3.0.0"
  },
  "devDependencies": {
    "angular2": "2.0.0-beta.0",
    "babel-polyfill": "6.3.14",
    "browser-sync": "2.11.0",
    "chai": "3.4.1",
    "del": "2.2.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-html-replace": "1.5.5",
    "gulp-hub": "0.7.1",
    "gulp-notify": "2.2.0",
    "gulp-plumber": "1.0.1",
    "gulp-replace": "0.5.4",
