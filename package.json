    "gulp-typescript": "2.10.0",
    "jspm": "0.16.23",
    "karma": "0.13.19",
    "karma-chai": "0.1.0",
    "karma-jspm": "2.0.2",
    "karma-mocha": "0.2.1",
    "karma-mocha-reporter": "1.1.5",
    "karma-phantomjs-launcher": "0.2.3",
